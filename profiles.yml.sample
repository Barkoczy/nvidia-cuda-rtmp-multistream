# Stream profiles configuration
# Streaming keys are loaded from environment variables using naming convention: PROFILE_SERVICE_KEY
# Example: For profile "gaming" and service "youtube", the key will be loaded from GAMING_YOUTUBE_KEY environment variable

gaming:
  youtube:
    url: rtmp://a.rtmp.youtube.com/live2
    bitrate: 40000k
    framerate: 60
    gopSize: 120
    preset: fast
    profile: main
  twitch:
    url: rtmp://live.twitch.tv/app
    bitrate: 8000k
    framerate: 60
    gopSize: 120
    preset: fast
    profile: high
    scale: 1920:1080
  kick:
    url: rtmp://rtmp.kick.com/live
    bitrate: 8000k
    framerate: 60
    gopSize: 120
    preset: fast
    profile: high
    scale: 1920:1080

events:
  youtube:
    url: rtmp://a.rtmp.youtube.com/live2
    bitrate: 40000k
    framerate: 60
    gopSize: 120
    preset: fast
    profile: main